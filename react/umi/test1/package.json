{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@ant-design/pro-descriptions": "^1.0.1",
    "@ant-design/pro-layout": "^6.14.0",
    "@ant-design/pro-table": "^2.0.1",
    "@umijs/preset-react": "1.x",
    "antd": "^4.12.2",
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "react-router": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "react-use": "^17.1.0",
    "umi": "^3.3.7",
    "umi-request": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^2.5.6",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.3.7",
    "eslint-webpack-plugin": "^2.5.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
